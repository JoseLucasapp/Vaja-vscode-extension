{
  "name": "zumbra-lang-support",
  "displayName": "ZUMBRA-LANG Support",
  "description": "Syntax highlighting and support for the ZUMBRA programming language",
  "version": "0.0.4",
  "publisher": "joselucasapp",
  "icon": "images/zumbra-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoseLucasapp/Zumbra-vscode-extension"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zumbra",
        "aliases": [
          "ZUMBRA",
          "zumbra"
        ],
        "extensions": [
          ".zum"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zumbra",
        "scopeName": "source.zumbra",
        "path": "./syntaxes/zumbra.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "zumbra-icons",
        "label": "ZUMBRA Icons",
        "path": "./icons/zumbra-icon-theme.json"
      }
    ]
  }
}