{
  "name": "vaja-lang-support",
  "displayName": "VAJA-LANG Support",
  "description": "Syntax highlighting and support for the VAJA programming language",
  "version": "0.0.1",
  "publisher": "joselucasapp",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoseLucasapp/Vaja-vscode-extension"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vaja",
        "aliases": [
          "VAJA",
          "vaja"
        ],
        "extensions": [
          ".vaja"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vaja",
        "scopeName": "source.vaja",
        "path": "./syntaxes/vaja.tmLanguage.json"
      }
    ]
  }
}